{
  "name": "auto-dining",
  "version": "0.0.0",
  "author": "Baraa Alkhatib",
  "description": "Angular 10 based application to find and share customer reviews of restaurants",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "node dist/server.js",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build-server": "webpack --mode production --config webpack.server.config.js --progress --color",
    "build-all": "npm run build-server && npm run build:prod",
    "heroku-postbuild": "npm run build-all",
    "dev": "ng build --watch",
    "dev-server": "webpack --mode development --config webpack.server.config.js --watch --progress --color",
    "watch": "nodemon dist/index.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "body-parser": "^1.19.0",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-eslint/builder": "0.8.0-beta.0",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.0",
    "@angular-eslint/schematics": "^0.8.0-beta.0",
    "@angular-eslint/template-parser": "0.8.0-beta.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-loader": "^8.0.11",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
